<<={data.yaml as d}
<html>
  <head>
    <title>${d.title}</title>
  </head>
  <body>
    <h1>Hello ${d.name}</h1>
    +{d.h1}
    ${d.h1}
    <p>My preferences are:</p>
    <table>
      *{d.prefs using each_pair as key, value}{
        <tr>
	        <td>${key}</td>
	        <td>${value}</td>
        </tr>
      }
    </table>
  </body>
</html>
